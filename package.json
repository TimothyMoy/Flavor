{
  "name": "flavor",
  "version": "1.0.0",
  "description": "Flavor is a recipe app that users can add or view recipes",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix flavor-app",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimothyMoy/Flavor.git"
  },
  "author": "Timothy Moy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TimothyMoy/Flavor/issues"
  },
  "homepage": "https://github.com/TimothyMoy/Flavor#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
